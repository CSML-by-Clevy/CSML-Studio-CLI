{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "prefer-template": "error",
    "camelcase": 0,
    "consistent-return": 0,
    "arrow-parens": 0,
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^_*",
        "argsIgnorePattern": "^_*"
      }
    ],
    "no-underscore-dangle": 0
  }
}
